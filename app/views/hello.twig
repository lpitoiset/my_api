
{% extends "template.twig" %}

{% block title %}{{name}}{% endblock %}
{% block head %}
    {{ parent() }}
    <style type="text/css">
        .important { color: #336699; }
    </style>
{% endblock %}
{% block content %}
	<h1>{{ "now"|date('d/m/Y H:i', timezone="Europe/Paris") }}</h1>
	Bonjour {{ name }}
	<br>
	{% for i in range(0, 3) %}
		{{ i }},<br>
	{% endfor %}
	<p>
		{% for i in range(low=1, high=10, step=2) %}
			{{ i }},
		{% endfor %}
	</p>
	URL:  <p class="important">
        hello
    </p>
{% endblock %}